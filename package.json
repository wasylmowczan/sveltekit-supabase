{
  "name": "launch-leopard",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "npm run sitemap:generate && vite build",
    "preview": "vite preview",
    "start": "node build",
    "sitemap:generate": "tsx src/lib/generateSitemap.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "db:generate": "drizzle-kit generate:pg",
    "push": "drizzle-kit push:pg",
    "pull": "drizzle-kit pull:pg",
    "db:migrate": "tsx src/lib/server/database/migrate.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.9.0",
    "daisyui": "^4.0.7",
    "nodemailer": "^6.9.4",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "svelte": "^4.0.5",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.4.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "type": "module",
  "dependencies": {
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/auth-ui-svelte": "^0.2.7",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.2",
    "@sveltejs/adapter-node": "^5.0.0",
    "@tailwindcss/forms": "^0.5.6",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "loops": "^2.0.0",
    "lucide-svelte": "^0.379.0",
    "openai": "^4.16.1",
    "postcss": "^8.4.29",
    "postgres": "^3.3.5",
    "sse.js": "^2.0.0",
    "stripe": "^15.0.0",
    "tailwindcss": "^3.3.3"
  }
}
